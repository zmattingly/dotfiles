[user]
	name = Zane Mattingly
	email = z@matting.ly
[core]
	autocrlf = input
	safecrlf = true
	excludesfile = /Users/z/.gitignore_global
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
